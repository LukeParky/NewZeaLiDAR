{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "standard": {
      "format": "%(asctime)s - %(levelname)s - %(name)s - %(module)s::%(funcName)s - %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "standard",
      "stream": "ext://sys.stdout"
    },
    "info_file_handler": {
      "class": "logging.handlers.RotatingFileHandler",
      "level": "INFO",
      "formatter": "standard",
      "filename": "logs/info.log",
      "maxBytes": 10485760,
      "backupCount": 20,
      "encoding": "utf8"
    },
    "error_file_handler": {
      "class": "logging.handlers.RotatingFileHandler",
      "level": "ERROR",
      "formatter": "standard",
      "filename": "logs/errors.log",
      "maxBytes": 10485760,
      "backupCount": 20,
      "encoding": "utf8"
    }
  },
  "loggers": {
    "py.warnings": {
      "level": "ERROR",
      "handlers": [
        "console"
      ],
      "propagate": false
    },
    "distributed": {
      "level": "WARNING",
      "handlers": [
        "info_file_handler"
      ],
      "propagate": false
    },
    "distributed.client": {
      "level": "WARNING",
      "handlers": [
        "info_file_handler"
      ],
      "propagate": false
    },
    "distributed.worker": {
      "level": "WARNING",
      "handlers": [
        "info_file_handler"
      ],
      "propagate": false
    },
    "scrapy": {
      "level": "WARNING",
      "handlers": [
        "console",
        "info_file_handler",
        "error_file_handler"
      ],
      "propagate": false
    },
    "newzealidar.catchments": {
      "level": "DEBUG",
      "handlers": [
        "console",
        "info_file_handler",
        "error_file_handler"
      ],
      "propagate": false
    },
    "newzealidar.datasets": {
      "level": "INFO",
      "handlers": [
        "console",
        "info_file_handler",
        "error_file_handler"
      ],
      "propagate": false
    },
    "newzealidar.datasets_waikato": {
      "level": "INFO",
      "handlers": [
        "console",
        "info_file_handler",
        "error_file_handler"
      ],
      "propagate": false
    },
    "newzealidar.lidar": {
      "level": "INFO",
      "handlers": [
        "console",
        "info_file_handler",
        "error_file_handler"
      ],
      "propagate": false
    },
    "newzealidar.lidar_waikato": {
      "level": "INFO",
      "handlers": [
        "console",
        "info_file_handler",
        "error_file_handler"
      ],
      "propagate": false
    },
    "newzealidar.process": {
      "level": "INFO",
      "handlers": [
        "console",
        "info_file_handler",
        "error_file_handler"
      ],
      "propagate": false
    },
    "newzealidar.tables": {
      "level": "INFO",
      "handlers": [
        "console",
        "info_file_handler",
        "error_file_handler"
      ],
      "propagate": false
    },
    "newzealidar.utils": {
      "level": "INFO",
      "handlers": [
        "console",
        "info_file_handler",
        "error_file_handler"
      ],
      "propagate": false
    }
  },
  "root": {
    "level": "INFO",
    "handlers": [
      "console",
      "info_file_handler",
      "error_file_handler"
    ]
  },
  "captureWarnings": true
}
